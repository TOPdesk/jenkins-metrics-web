include mixins/dependencies

doctype html
head
  +javascriptLibraries
  +styles
body
  +chartGlobals
  div.container
    aside#jobs.left.expanded
      header
        button#jobs-collapse
          span ❌
        button#jobs-expand
          span ☰
      nav
        ul
          each job in jobs
            li(data-job="#{job}")
              a(href="#!#{job}") #{job}
    main#content.right
