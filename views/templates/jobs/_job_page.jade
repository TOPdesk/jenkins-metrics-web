include ../../mixins/scripts

header
  h1
    a#previous-job(href="#") ◀
    span #{name}
    a#next-job(href="#") ▶

nav.pages
  ul
    li
      a(href="#!#{name}",class="#{page === 'default' ? 'active' : ''}") Build times
    li
      a(href="#!#{name}/tests",class="#{page === 'tests' ? 'active' : ''}") Test results

block content

+onDocumentReady
  include ../scripts/updateNavigationLinks.js
  | updateNavigationLinks('#{name}');
